{
  "name": "dvsa/authentication-ldap",
  "type": "library",
  "description": "A wrapper around LDAP",
  "keywords": ["dvsa", "authentication", "ldap"],
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "ext-ldap": "*",
    "ext-openssl": "*",
    "dvsa/contracts": "^1.1",
    "firebase/php-jwt": "^6.3",
    "illuminate/support": "^6.20 || ^8.49",
    "league/oauth2-client": "^2.6.1",
    "nesbot/carbon": "^2.50",
    "symfony/ldap": "^5.3"
  },
  "conflict": {
    "guzzlehttp/guzzle": "<6.5.8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Dvsa\\Authentication\\Ldap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dvsa\\Authentication\\Ldap\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8.2",
    "phpunit/phpunit": "^9.5.4"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  },
  "extra" : {
    "bamarni-bin": {
      "bin-links": true,
      "forward-command": true
    }
  }
}
