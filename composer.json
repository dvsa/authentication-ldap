{
  "name": "dvsa/authentication-ldap",
  "type": "library",
  "description": "A wrapper around LDAP",
  "keywords": [
    "dvsa",
    "authentication",
    "ldap"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-ldap": "*",
    "ext-openssl": "*",
    "dvsa/contracts": "^1.3",
    "firebase/php-jwt": "^6.3",
    "illuminate/support": "^8.49 || ^11.0",
    "league/oauth2-client": "^2.6.1",
    "nesbot/carbon": "^2.63 || ^3.4",
    "symfony/ldap": "^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Dvsa\\Authentication\\Ldap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dvsa\\Authentication\\Ldap\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8.2",
    "phpunit/phpunit": "^11.1"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": true,
      "forward-command": true
    }
  }
}
